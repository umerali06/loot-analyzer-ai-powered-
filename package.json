    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.540.0",
    "mongodb": "^6.3.0",
    "next": "14.0.4",
    "openai": "^5.13.1",
    "postcss": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.3.8",
    "redis": "^4.6.11",
    "sharp": "0.33.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  },
